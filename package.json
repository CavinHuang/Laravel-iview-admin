        "laravel-mix": "^1.0",
        "lodash": "^4.17.4",
        "vue": "^2.5.7",
        "iview": "^2.7.2",
        "less": "^2.7.1",
        "less-loader": "^2.2.3",
        "js-cookie": "^2.1.3",
        "vue-router": "^2.8.1",
        "vuex": "^2.5.0"
    }
